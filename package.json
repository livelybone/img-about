{
  "name": "img-about",
  "version": "2.1.0",
  "description": "A module for image processing in browser, includes getting naturalSize, zooming, compressing... wrapped by Promise",
  "main": "./lib/umd/index.js",
  "module": "./lib/es/index.js",
  "unpkg": "./lib/umd/index.js",
  "scripts": {
    "build": "rimraf ./lib && cross-env NODE_ENV=production rollup -c",
    "eslint": "eslint ./src --fix",
    "test": "npm run build && mocha ./test/index.spec.js",
    "changelog": "npx conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/livelybone/img-about.git"
  },
  "keywords": [
    "image",
    "img",
    "naturalSize",
    "naturalWidth",
    "naturalHeight",
    "zoom",
    "scale",
    "compress",
    "browser"
  ],
  "author": "livelybone(2631541504@qq.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/livelybone/img-about/issues"
  },
  "homepage": "https://livelybone.github.io/tool/img-about/",
  "devDependencies": {
    "commitizen": "^3.0.7",
    "conventional-changelog-cli": "^2.0.21",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "mocha": "^5.2.0",
    "rollup": "^1.9.0",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-license": "^0.8.1",
    "rollup-plugin-node-resolve": "^4.2.1",
    "rollup-plugin-uglify": "^6.0.2"
  },
  "dependencies": {
    "@livelybone/copy": "^2.5.4",
    "base64-blob": "^1.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
